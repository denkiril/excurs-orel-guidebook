{
  "name": "orel-guidebook",
  "version": "0.9.42",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/setenv.ts",
    "start": "npm run build:dev && ng serve --configuration=development --port=4201",
    "build": "ng build",
    "build:dev": "npm run config -- --environment=dev",
    "build:prod": "npm run config -- --environment=prod && ng build --configuration=production --base-href /guidebook/dist/exogb/browser/",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy1": "scp -r deploy/. denkiril@denkiril.beget.tech:~/dev.excurs-orel.ru/public_html/",
    "deploy2": "scp -r dist/guidebook/. denkiril@denkiril.beget.tech:~/dev.excurs-orel.ru/public_html/guidebook/",
    "deploy": "npm run deploy1 && npm run deploy2",
    "dev:ssr": "npm run build:dev && ng run orel-guidebook:serve-ssr",
    "serve:ssr": "node dist/exogb/server/main.js",
    "build:ssr": "npm run build:prod && ng run orel-guidebook:server",
    "prerender": "ng run orel-guidebook:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.8",
    "@angular/common": "16.2.8",
    "@angular/compiler": "16.2.8",
    "@angular/core": "16.2.8",
    "@angular/forms": "16.2.8",
    "@angular/platform-browser": "16.2.8",
    "@angular/platform-browser-dynamic": "16.2.8",
    "@angular/platform-server": "16.2.8",
    "@angular/router": "16.2.8",
    "@nguniversal/express-engine": "^16.2.0",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.5",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
    "@angular-eslint/schematics": "^16.2.0",
    "@angular-eslint/template-parser": "^16.2.0",
    "@angular/cli": "^16.2.5",
    "@angular/compiler-cli": "^16.2.8",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^3.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5",
    "yargs": "^17.5.1"
  }
}
