    "@tinkoff/eslint-config": "^1.54.4",
    "@tinkoff/eslint-config-angular": "^1.54.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "dotenv": "^16.0.1",
